<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.fu.dao.FileDAO" >
<!-- 文件映射表 -->
  <resultMap id="BaseResultMap" type="com.fu.entity.File" >
    <id column="id" property="id" jdbcType="INTEGER" />
      <result column="file_f" property="file_f" jdbcType="BINARY" />
  </resultMap>
  <!-- 添加文件  -->
  <insert id="addFile">
  insert into file_f(file_f.file_f,file_f.fname) values (#{b},#{fname})

  </insert>

    
    
</mapper>